<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userMapper">
	<select id="chkInputId" parameterType="String" resultType="int">
		SELECT COUNT(ID)
		FROM AAA_MEMBER
		WHERE ID = #{inputId}
	</select>
</mapper>